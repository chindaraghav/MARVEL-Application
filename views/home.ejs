                        <span>
                            <h3 style="font-family:Encode Sans Condensed; font-weight: bold;" class="text-center"><%=data.movies[i].title%></h3>

                        <a href="/movie/<%=data.movies[i].id%>"><p style="color: blueviolet; font-size: 17px;">View More<i class="fa fa-arrow-right" aria-hidden="true"></i></p></a>
                        </span>

                    </div>
                </div>
            </div>
            <%}%>
        </div>
    </div>

</div>
</div>
