<body>


    <div id="head" class="img-responsive"></div>
    <div id="head2" class="img-responsive">
        <img src="/stylesheets/logo3.jpg" alt="">
    </div>
    <%-include('navB')%>
    <div style="background:black" class="container-fluid">
<div>

    <div id="cont" class="container" >
        <div id="image-con">

            <%for(var i=0;i<data.movies.length;i++){%>
            <div class="conA">
                <div class="ibox col-md-4 col-xs-12 img-responsive">
                    <img class="imageS " src="<%=data.movies[i].image%>" alt="">
